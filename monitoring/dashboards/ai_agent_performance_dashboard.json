{
  "dashboard": {
    "id": null,
    "title": "AI Agent Performance Dashboard",
    "tags": ["ai-agent", "performance", "monitoring"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Agent Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(agent_requests_total[5m])",
            "legendFormat": "{{type}} requests/sec",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "yAxes": [
          {
            "format": "reqps",
            "label": "Requests/sec"
          }
        ]
      },
      {
        "id": 2,
        "title": "Agent Request Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 {{type}}",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.5, rate(agent_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P50 {{type}}",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "yAxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          }
        ]
      },
      {
        "id": 3,
        "title": "Agent Confidence Score",
        "type": "graph",
        "targets": [
          {
            "expr": "avg(agent_confidence_score) by (type)",
            "legendFormat": "{{type}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8},
        "yAxes": [
          {
            "format": "percentunit",
            "label": "Confidence"
          }
        ]
      },
      {
        "id": 4,
        "title": "Cognitive Load",
        "type": "graph",
        "targets": [
          {
            "expr": "cognitive_load_current",
            "legendFormat": "{{component}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8},
        "yAxes": [
          {
            "format": "percentunit",
            "label": "Load"
          }
        ]
      },
      {
        "id": 5,
        "title": "Memory Entries",
        "type": "graph",
        "targets": [
          {
            "expr": "memory_entries_total",
            "legendFormat": "{{memory_type}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "yAxes": [
          {
            "format": "short",
            "label": "Entries"
          }
        ]
      },
      {
        "id": 6,
        "title": "Learning Success Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "learning_success_rate",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "yAxes": [
          {
            "format": "percentunit",
            "label": "Success Rate"
          }
        ]
      },
      {
        "id": 7,
        "title": "Tool Call Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tool_calls_total[5m])",
            "legendFormat": "{{tool_name}} - {{status}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 24},
        "yAxes": [
          {
            "format": "reqps",
            "label": "Calls/sec"
          }
        ]
      },
      {
        "id": 8,
        "title": "Tool Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tool_duration_seconds_bucket[5m]))",
            "legendFormat": "{{tool_name}} P95",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 24},
        "yAxes": [
          {
            "format": "s",
            "label": "Duration (seconds)"
          }
        ]
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "links": [],
    "gnetId": null
  }
}